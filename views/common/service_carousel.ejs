<div class="uk-width-1-3@m uk-width-1-1@s uk-margin-medium-bottom">
        <div class="uk-article uk-test-serve">
            <div class="uk-card uk-overflow-hidden uk-background-default">
                <div class="uk-visible-toggle uk-position-relative">
                    <div class="uk-card-media-top">
                        <a href="service/<%= item.slug %>">
                            <img data-src="images/placeholder.png" alt="" style="height:250px;" uk-img>
                        </a>
                    </div> <!-- uk-card-media-top -->
                    <div class="uk-overlay-primary uk-position-cover uk-hidden-hover">
                        <div class="uk-position-center">
                            <div class="uk-grid uk-grid-small uk-margin-remove">
                                <div class="uk-width-auto uk-padding-remove">
                                <a href="#"><i class="overlay-icon fa fa-heart-o"></i></a>
                                </div>
                                <div class="uk-width-expand uk-margin-remove">
                                <a href="service/<%= item.slug %>"> <i class="overlay-icon fa fa-long-arrow-right"></i></a>
                                </div>
                            </div>
                        </div> <!-- uk-position center -->
                    </div> <!-- uk-overlay primary -->
                </div> <!-- uk-visibilty -->
                <span class="uk-label uk-position-absolute">
                    <a href="search?category<%= item && item.category && item.category.id? item.category.id : 
                    ''%>">
                        <%= item.category?item.category.name: "" %>
                    </a>
                </span>
                <div class="uk-card-body uk-padding-small uk-margin-small-top">
                    <h3 class="uk-card-title uk-margin-remove-top">
                        <a href="service/<%= item.slug %>">
                            <%= item.title %>
                        </a>
                    </h3>
                    <p class="uk-text-emphasis uk-margin-small"><%= item && item.city && item.city.name ? item.city.name : "" %>, <%= item && item.state && item.state.name ? item.state.name : ""  %> | <del><del>Distance : 2 KM</del></del> </p>
                    <p class="uk-margin-remove-top">Compansation : <%= item && item.compensation ? item.compensation : ""  %></p>
                </div> <!-- uk-card-body -->
                <div class="uk-margin-remove-left service-meta uk-child-width-expand uk-grid">
                    <div class="uk-width-expand uk-padding-remove-left first-child">
                        <div class="uk-grid-small uk-margin-remove uk-flex-middle uk-padding-small uk-grid">
                            <div class="review uk-padding-remove">
                               
                                <p class="uk-text-meta uk-margin-remove uk-display-inline-block">(<%= item.ranking %>) hits(<%= item.hits %>) </p>
                            </div>
                        </div>
                    </div>
                   
                      <div class="uk-width-auto uk-padding-remove">                          
                                <div class="hover-icon uk-text-center">                               
                                <a :syncing="fvalue"  class="uk-padding-small uk-display-inline-block" 
                                     href=""  @click="fav('<%=item.id%>')">
                                    <span slot="syncing-state">
                                        <span v-if="fvalue"><i class="fa fa-heart"></i> </span>
                                        <span v-else><i class="fa fa-heart-o"></i> </span>
                                    </span>                              
                                </a>  
                                </div>     
                            </div>
                    <div class="uk-width-auto uk-padding-remove">
                        <!--  update 1-->
                        <div class="uk-text-center">
                        <a class="uk-padding-small read-more uk-display-block" href="service/<%=item.id%>">Read More</a>
                        </div><!-- uk-text-center -->
                    </div> <!-- uk-width-1-3@m -->
                </div> <!-- service-meta -->
            </div> <!-- uk-card -->
        </div><!-- uk-article -->
    </div> <!-- uk-width-1-3@m -->
        
        