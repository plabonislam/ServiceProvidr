<section id="feature" class="feature uk-background-defaut">
    <div class="uk-container">
        <div class="slider-items uk-slider uk-position-relative">
            <div class="uk-slider uk-slider-container" data-uk-slider="autoplay: false; finite: false; autoplay-interval: 3000;" tabindex="-1">

                <ul class="uk-slider-items uk-child-width-1-5@l uk-child-width-1-2 uk-child-width-1-5@m  uk-height" 
                    data-uk-height-match="target: > li > .uk-card; row: true">

                    <% data.category.forEach(function(item){ %>
                        <li>
                            <div class="uk-card uk-card-default uk-text-center uk-padding-small   uk-flex uk-flex-center uk-flex-middle category-list-home">
                                <a href="search/<%= item.slug%>">
                                    <svg width="65" height="65" viewBox="0 0 20 20" xmlns="http://www.w3.org/2000/svg" data-svg="nut"><polygon fill="none" stroke="#ff9833" points="2.5,5.7 10,1.3 17.5,5.7 17.5,14.3 10,18.7 2.5,14.3"></polygon><circle fill="none" stroke="#ff9833" cx="10" cy="10" r="3.5"></circle></svg>
                                    <p class="uk-text-lead uk-margin-remove-bottom"><%= item.name %></p>
                                </a>
                            </div>                                    
                        </li>
                    <% }); %>
                </ul>
                <div class="uk-visible@s">
                    <a class="uk-position-center-left-out" href="#" uk-slidenav-previous uk-slider-item="previous"></a>
                    <a class="uk-position-center-right-out" href="#" uk-slidenav-next uk-slider-item="next"></a>
                </div>
                <ul class="uk-slider-nav uk-hidden@m uk-hidden@l uk-hidden@xl uk-dotnav uk-flex-center uk-margin"></ul>
            </div> <!-- data-uk-slider -->
        </div> <!-- slider-items -->          
    </div> <!-- uk-container -->
</section>
<!-- End Service --> 